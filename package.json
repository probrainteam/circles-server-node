  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.8",
    "express": "^4.17.2",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "scripts": {
    "start" : "node dist/app.js",
    "build" : "tsc -p .",
    "main": "nodemon --watch \"src/**/*.ts\" --exec \"ts-node\" src/app.ts main",
    "dev":  "echo \"starting in dev\" && nodemon --watch \"src/**/*.ts\" --exec \"ts-node\" src/app.ts dev"
  }
}
