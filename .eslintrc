{
    "env":{
        "node": true
    },
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended", "prettier"
    ],
    "rules": { 
        "no-console": "warn",
        "no-unused-vars":"off"
    }
}